{
  "name": "irs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve & (export $(cat .env.development | xargs) && nodemon ./server/index.js)",
    "build": "npm run get-api-types && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "start": "node ./server/index.js",
    "pick-services": "ts-node --compiler-options '{\"module\":\"commonjs\"}' ./scripts/pick-services.ts",
    "schema": "typescript-json-schema \"src/types/*ts\" \"Requests\" > src/types/schema.json",
    "get-api-types": "export $(cat .env.development | xargs) && curl -L -v --insecure -H \"Accept: application/json\" \"$VUE_APP_API_HOST/apis/swagger/schema\" > ./api-spec.json && openapi -i api-spec.json -o ./src/api && npm run pick-services WorkflowService ResearchService MeService"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@mdi/font": "^3.6.95",
    "@types/lodash": "^4.14.165",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "dexie": "^3.0.3",
    "express": "^4.17.1",
    "jaro-winkler": "^0.2.8",
    "lodash": "^4.17.20",
    "neat-csv": "^5.2.0",
    "node-random-name": "^1.0.1",
    "register-service-worker": "^1.7.1",
    "roboto-fontface": "*",
    "socket.io": "^3.1.1",
    "socket.io-client": "^3.1.1",
    "typescript-json-schema": "^0.43.0",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.5.1",
    "vue-router": "^3.2.0",
    "vue-virtual-table": "^0.2.17",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "chai": "^4.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.7",
    "openapi-typescript-codegen": "^0.7.2",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "schema-dts": "^0.8.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "~3.9.3",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "^2.6.11"
  }
