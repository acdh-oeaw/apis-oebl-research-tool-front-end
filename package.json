    "register-service-worker": "^1.7.1",
    "roboto-fontface": "*",
    "skeleton-loader": "^2.0.0",
    "socket.io": "^3.1.1",
    "socket.io-client": "^3.1.1",
    "tippy.js": "^6.3.1",
    "tslib": "^2.1.0",
    "typescript-json-schema": "^0.43.0",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.5.1",
    "vue-router": "^3.2.0",
