{
	"name": "@acdh-oeaw/irs",
	"private": true,
	"license": "MIT",
	"engines": {
		"node": "16.x",
		"npm": "8.x"
	},
	"packageManager": "npm@8.19.4",
	"scripts": {
		"build": "run-s generate:api-client:prod build:client build:server",
		"build:client": "vue-cli-service build",
		"build:server": "tsc --outDir ./server/dist/ ./server/index.ts",
		"dev": "run-p dev:client dev:server",
		"dev:client": "vue-cli-service serve",
		"dev:server": "env-cmd --silent -f .env.local ts-node-dev --project ./server/tsconfig.json ./server/index.ts",
		"generate:api-client": "ts-node ./scripts/get-api-spec.ts && openapi -i api-spec.json -o ./src/api --request ./src/service/request.ts && npm run pick-services WorkflowService ResearchService MeService",
		"generate:api-client:dev": "env-cmd --silent -f .env.development npm run generate:api-client",
		"generate:api-client:prod": "env-cmd --silent -f .env.production npm run generate:api-client",
		"lint:check": "vue-cli-service lint --no-fix",
		"lint:fix": "vue-cli-service lint",
		"pick-services": "ts-node ./scripts/pick-services.ts",
		"prepare": "npm run generate:api-client:prod",
		"start": "env-cmd --silent -f .env.local node ./server/dist/server/index.js",
		"test": "NODE_ENV=production vue-cli-service test:e2e --headless --env PORT=8080",
		"test:dev": "NODE_ENV=development vue-cli-service test:e2e --mode=development --headless  --env PORT=8080",
		"test:interactive": "NODE_ENV=production vue-cli-service test:e2e --env PORT=8080",
		"test:interactive:dev": "NODE_ENV=development vue-cli-service test:e2e --mode=development --env PORT=8080",
		"test:unit": "vue-cli-service test:unit",
		"types:check": "tsc --noEmit",
		"validate": "run-p lint:check types:check test:unit test"
	},
	"dependencies": {
		"@babel/polyfill": "^7.12.1",
		"@mdi/font": "^3.6.95",
		"@tiptap/extension-highlight": "^2.0.0-beta.12",
		"@tiptap/starter-kit": "^2.0.0-beta.59",
		"@tiptap/vue-2": "^2.0.0-beta.29",
		"body-parser": "^1.20.2",
