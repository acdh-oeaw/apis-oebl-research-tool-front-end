	"browserslist": {
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		],
		"production": [
			">=0.75%",
			"not dead",
			"not op_mini all"
		]
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"ts",
			"json",
			"vue"
		],
		"transform": {
			".*\\.(vue)$": "vue-jest",
			"^.+\\.tsx?$": "ts-jest"
		},
		"testURL": "http://localhost/:8080"
	},
	"prettier": "@acdh-oeaw/prettier-config",
	"volta": {
		"node": "16.20.1"
	}
}
